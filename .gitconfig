[user]
	name = Alex Kurihara
	email = akurihara@spotify.com
[color]
    ui = true
[alias]
    co = checkout 
    cob = checkout -b
    b = branch
    l = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
[core]
    editor = /usr/local/bin/vim
	  excludesfile = /Users/alexkurihara/.global_ignore
[push]
    default = simple
[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
[maintenance]
	repo = /Users/akurihara/Developer/services-pilot
